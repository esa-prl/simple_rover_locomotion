ros2 topic pub -r 1 /simple_rover_locomotion/rover_velocities geometry_msgs/TwistStamped "{header: {stamp: [1], frame_id: 'bla'},  twist: {linear: {x: 1.0, y: 2.0, z: 3.0}, angular:  {x: 1.0 ,y: 1.0 ,z: 1.0}}}"

ros2 topic pub /simple_rover_locomotion/rover_velocities geometry_msgs/TwistStamped "{header: auto,  twist.linear: {x: 1, y: 2, z: 3}, twist.angular: {x: 1,y: 1,z: 1}}"

ros2 topic pub /chatter std_msgs/String "data: Hello ROS Developers"